<?xml version="1.0"?>
<device>
  <developer>AGH</developer>
  <product>soil00</product>
  <pwrdownmode>true</pwrdownmode>
  <regular>
    <reg name="Soil Temperature" id="11">
      <endpoint name="soil_temp" type="num" dir="inp">
        <position>0</position>
        <size>2</size>
        <units>
          <unit name="C" factor="0.1" offset="-50"/>
        </units>
      </endpoint>
    </reg>
	<reg name="Soil Humidity" dir="12">
		<endpoint name="soil_hum" type="num" dir="inp">
			<position>0</position>
			<size>2</size>
			<units>
 			<unit name="kPa" factor="0.286" offset="0" />
			</units>
		</endpoint>
	</reg>
  </regular>
</device>
